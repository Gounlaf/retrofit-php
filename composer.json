{
    "name": "tebru/retrofit-php",
    "description": "Retrofit for PHP - A PHP REST client.",
    "require": {
        "php": ">= 5.4",
        "jms/serializer": "^1.0",
        "phpoption/phpoption": "^1.1",
        "symfony/console": "^2.3|^3.0",
        "tebru/assert": "^0.2",
        "tebru/dynamo": "^0.3.1",
        "tebru/retrofit-http-clients": "^0.6.6",
        "symfony/event-dispatcher": "^2.3|^3.0",
        "guzzlehttp/psr7": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.0",
        "guzzlehttp/guzzle": "^5.3|^6.0",
        "behat/behat": "^3.1",
        "tebru/retrofit-test-api": "~0.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nate Brunette",
            "email": "n@tebru.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tebru\\Retrofit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tebru\\Retrofit\\Test\\": "tests/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to make requests"
    },
    "bin": ["bin/retrofit"]
}
